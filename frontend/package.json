{
    "name": "zenoml",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "npm run smui-theme && vite build",
        "smui-theme": "smui-theme compile public/smui.css -i src/theme",
        "format": "npx prettier -w src",
        "lint": "npx eslint src --max-warnings=0 && npx prettier -c src",
        "test": "npx jest tests",
        "check": "svelte-check",
        "generate-api": "npx openapi-typescript-codegen --input http://localhost:8000/api/openapi.json --output ./src/zenoservice --client fetch; npx prettier -w ./src/zenoservice"
    },
    "devDependencies": {
        "@formkit/auto-animate": "^1.0.0-beta.5",
        "@smui/button": "7.0.0-beta.8",
        "@smui/card": "7.0.0-beta.3",
        "@smui/checkbox": "7.0.0-beta.8",
        "@smui/chips": "7.0.0-beta.3",
        "@smui/circular-progress": "7.0.0-beta.8",
        "@smui/data-table": "7.0.0-beta.8",
        "@smui/dialog": "7.0.0-beta.8",
        "@smui/form-field": "7.0.0-beta.8",
        "@smui/layout-grid": "7.0.0-beta.3",
        "@smui/list": "7.0.0-beta.3",
        "@smui/paper": "7.0.0-beta.8",
        "@smui/ripple": "7.0.0-beta.3",
        "@smui/segmented-button": "^7.0.0-beta.3",
        "@smui/select": "7.0.0-beta.8",
        "@smui/slider": "^7.0.0-beta.3",
        "@smui/textfield": "7.0.0-beta.3",
        "@svelte-plugins/tooltips": "^0.1.6",
        "@sveltejs/vite-plugin-svelte": "1.4.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/svelte": "^3.2.2",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.1",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "eslint": "^8.39.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "node-html-parser": "^6.1.5",
        "openapi-typescript-codegen": "^0.24.0",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.8.1",
        "simple-svelte-autocomplete": "^2.5.2",
        "smui-theme": "7.0.0-beta.3",
        "svelecte": "^3.16.0",
        "svelte": "^3.56.0",
        "svelte-check": "^3.3.2",
        "svelte-loading-spinners": "^0.3.4",
        "svelte-preprocess": "^5.0",
        "tslib": "^2.4.1",
        "typescript": "^5.0.3",
        "vite": "^3.2.5"
    },
    "dependencies": {
        "@mdi/js": "^7.2.96",
        "d3-array": "^3.2.1",
        "d3-interpolate": "^3.0.1",
        "d3-scale": "^4.0.2",
        "d3-scale-chromatic": "^3.0.0",
        "d3-selection": "^3.0.0",
        "d3-shape": "^3.1.0",
        "internmap": "^2.0.3",
        "regl-scatterplot": "^1.6.6",
        "sirv-cli": "^2.0.0",
        "svelte-dnd-action": "^0.9.22",
        "svelte-spa-router": "^3.3.0",
        "svelte-vega": "^2.0.0"
    },
    "prettier": {
        "tabWidth": 2,
        "useTabs": true,
        "bracketSameLine": true
    }
}